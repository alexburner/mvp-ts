    "url": "https://github.com/alexburner/mvp-ts/issues"
  },
  "homepage": "https://github.com/alexburner/mvp-ts#readme",
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "parcel-bundler": "1.12.3",
    "prettier": "1.17.1",
    "sass": "1.20.1",
    "typescript": "3.4.5"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "7.4.4",
    "components": "^0.1.0",
    "d3-interpolate": "^1.3.2",
